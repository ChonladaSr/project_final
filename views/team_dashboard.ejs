  <title>หน้าหลัก</title>
  <link rel="icon" href="https://cdn-icons-png.flaticon.com/512/6342/6342703.png">
  <%- include('./font') %>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
