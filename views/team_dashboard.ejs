    <script src="https://cdn.tailwindcss.com"></script>

    <%- include('./font') %>

      <style>
        body {
          margin: 0;
          font-family: Arial, Helvetica, sans-serif;
        }

        .topnav {
          overflow: hidden;
          background-color: #333;
        }

        .topnav a {
          float: left;
          color: #f2f2f2;
          text-align: center;
          padding: 14px 16px;
          text-decoration: none;
          font-size: 17px;
        }

        .topnav a:hover {
          background-color: #ddd;
          color: black;
        }

        .topnav a.active {
          background-color: #04AA6D;
          color: white;
        }

        .topnav-right {
          float: right;
        }
      </style>
</head>

<body>

  <div class="topnav prompt-medium">
    <a class="active" href="/team/dashboard">หน้าหลัก</a>
    <a href="/teams/get_pending_bookings">งานที่รอดำเนินการ</a>
    <a href="#team">งานที่กำลังซ่อมแซม</a>
    <a href="/team/get_all_bookings">งานทั้งหมด</a>
    <div class="topnav-right">
